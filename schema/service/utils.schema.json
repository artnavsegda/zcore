{
  "$schema": "http://json-schema.org/schema#",
  "title": "utils",
  "type": "object",
  "properties": {
  },
  "commands": {
    "ping": {
      "command": "/usr/bin/ping",
      "args": ["-c 10"],
      "argpass": true,
      "wait": true,
      "description": "ping command"
    },
    "env": {
      "command": "/usr/bin/env",
      "args": ["env"],
      "argpass": true,
      "wait": true,
      "global": true
    },
    "dump": {
      "command": "dump.sh",
      "args": ["dump.sh"],
      "json": "in",
      "reload": true
    },
    "save": {
      "command": "save.sh",
      "args": ["save.sh"],
      "json": "in",
      "reload": true
    }
  }
}

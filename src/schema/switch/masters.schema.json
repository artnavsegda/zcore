{
  "$schema": "http://json-schema.org/schema#",
  "title": "masters",
  "description": "switch master ports list",
  "type": "object",
  "namesake": "name",
  "patternProperties": {
    "^port": {
      "type": "object",
      "title": "HW port entity",
      "properties": {
        "name": {
          "type": "string",
          "hidden": true
        },

        "port": {
          "type": "number",
          "description": "PHY switch port",
          "readonly": true
        },
        
      }
    }
  },
  "commands": {
  },

  "acquire": {
     "shell": "/opt/zenith/script/get.sh",
     "args": ["get.sh", "switch-master-ports", "switch.masters.schema"]
   }
}

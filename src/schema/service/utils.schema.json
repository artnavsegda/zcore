{
  "$schema": "http://json-schema.org/schema#",
  "title": "utils",
  "type": "object",
  "namesake": "name",
  "properties": {
    "demo": {
      "type": "string",
      "description": "test option"
    },
    "subdemo": {
      "type": "object",
      "description": "test suboption",
      "properties": {
        "subthing" : {
          "type": "string",
          "description": "test sub thing"
        }
      }
    },
    "multisubdemo": {
      "type": "object",
      "properties": {
        "one": {
          "type": "object",
          "properties": {
            "two": {
              "type": "object",
              "properties": {
                "three": {
                  "type": "object",
                  "properties": {
                    "something": {
                    "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "commands": {
    "ping": {
      "command": "/usr/bin/ping",
      "args": ["ping", "-c 10"],
      "argpass": true,
      "wait": true
    },
    "env": {
      "command": "/usr/bin/env",
      "args": ["env"],
      "argpass": true,
      "wait": true
    },
    "dump": {
      "command": "dump.sh",
      "args": ["dump.sh"],
      "json": "in",
      "reload": true
    },
    "save": {
      "command": "save.sh",
      "args": ["save.sh"],
      "json": "in",
      "reload": true
    }
  },
  "acquire": {
     "file": "utils.json"
   }
}
